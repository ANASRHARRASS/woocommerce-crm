{
    "name": "your-vendor/woocommerce-crm",
    "description": "WooCommerce CRM Plugin: leads, forms, HubSpot & Zoho sync, order & shipping helpers.",
    "type": "wordpress-plugin",
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Anas\\WCCRM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anas\\WCCRM\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}