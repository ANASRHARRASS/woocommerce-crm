{
    "name": "anasrharrass/woocommerce-crm-plugin",
    "description": "A WordPress plugin that integrates WooCommerce with HubSpot and Zoho CRM, providing dynamic forms, order management, and lead capture from social media.",
    "type": "wordpress-plugin",
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "WooCommerceCRMPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WooCommerceCRMPlugin\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
